<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Test Suite with Chart Reporter">
    
    <!-- Register the custom HTML bar chart reporter -->
    <listeners>
<listener class-name="FO.CustomGraphReporter"/>
         <listener class-name="FO.ExcelTestListener"/>
         <listener class-name="FO.ChartReporter"/>
    </listeners>

    <!-- Define the tests to run -->
    <test name="Login Test Cases">
        <classes>
            <class name="FO.Login"/>
             <class name="FO.Reservation"/>
            <class name="FO.Reservation_checkIN"/>
            <class name="FO.Advance_distribution"/>
        </classes>
    </test>
</suite>